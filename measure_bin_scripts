for x in bin/*
do 
	# time $(basename $x); 
	# time $(basename $x) > /dev/null 2>&1 &

	# start=`date +%s%N | cut -b1-13`

	start=`gdate +%s%N | cut -b1-13`
	# $(basename $x) > somefile 2>&1 &
	# $(basename $x);
	sleep 1.5
	end=`gdate +%s%N | cut -b1-13`
	
	# runtime=$((end-start))

	runtime=`echo "scale=3;($end - $start)/1000" | bc -l`
	echo $(basename $x),  $start, $end, $runtime
done